paths = ['../src/clustering/*']
flags = ['-Werror', '-g']
libs = ['opencv_core', 'opencv_highgui', 'opencv_imgproc', 'opencv_nonfree', 'opencv_features2d']
libpaths = []

#Source files
clustering_sources = Glob('../src/clustering/*.cc')

Program('../bin/clustering', clustering_sources, CPPPATH=paths, CPPFLAGS=flags, LIBPATH=libpaths, LIBS=libs)
